  "dependencies": {
    "@inquirer/prompts": "^2.0.0",
    "axios": "^1.2.3",
    "bcryptjs": "^2.4.3",
    "commander": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "nodemon": "^3.0.2",
    "sqlite3": "^5.1.6",
    "userop": "^0.3.4",
    "uuid": "^9.0.1"
